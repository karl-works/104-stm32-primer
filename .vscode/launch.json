    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug 01_blinky",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/app/01_blinky/01_blinky.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "interface": "swd",
            "device": "st_nucleo_g0.cfg",
            "configFiles": [
                "board/st_nucleo_g0.cfg"
            ],
            "preLaunchCommands": [
                "set mem inaccessible-by-default off",
                "monitor reset"
            ],
            "postLaunchCommands": [
                "monitor reset init",
                "monitor sleep 200"
            ]
